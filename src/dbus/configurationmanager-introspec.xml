      <arg type="s" name="tone" direction="out"/>
    </method>
		
    <method name="setRingtoneChoice">
      <arg type="s" name="tone" direction="in"/>
    </method>
		
    <method name="getAudioManager">
      <arg type="i" name="api" direction="out"/>
