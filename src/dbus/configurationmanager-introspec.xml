    <!-- Codecs-related methods -->
		
    <method name="getCodecList">
      <annotation name="com.trolltech.QtDBus.QtTypeName.Out0" value="VectorString"/>
      <arg type="as" name="list" direction="out"/>
    </method>
		
    <method name="getCodecDetails">
